{
    "name": "league_login_app",
    "productName": "League login app",
    "version": "1.0.0",
    "description": "An app to help you login to League of Legends",
    "author": "Hollitizz#0111",
    "scripts": {
        "build-dir": "vue-tsc --noEmit && vite build && electron-builder --dir",
        "build": "vue-tsc --noEmit && vite build && electron-builder",
        "dev": "vite",
        "preview": "vite preview",
        "increment-version": "git add . && git commit -m 'newversion' && git push",
        "build-and-publish": "npm run increment-version && npm run build -p"
    },
    "main": "dist-electron/main/index.js",
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@vitejs/plugin-vue": "^4.1.0",
        "bcryptjs": "^2.4.3",
        "electron": "^23.1.1",
        "electron-builder": "^23.6.0",
        "electron-forge": "^5.2.4",
        "typescript": "^5.0.2",
        "versiony": "^2.0.1",
        "vite": "^4.1.4",
        "vite-plugin-electron": "^0.11.2",
        "vite-plugin-electron-renderer": "^0.14.1",
        "vue": "^3.2.47",
        "vue-svg-loader": "^0.17.0-beta.2",
        "vue-tsc": "^1.2.0"
    },
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
        }
    },
    "keywords": [
        "electron",
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "license": "MIT",
    "dependencies": {
        "axios": "^1.4.0",
        "electron-updater": "^5.3.0",
        "encrypt-with-password": "^1.0.0",
        "pinia": "^2.0.35",
        "sass": "^1.62.1",
        "vue-toastification": "^2.0.0-rc.2"
    }
}
